.station{
	height:100%;
	width:100%;
	color:#fff;
	padding:1rem;
	font-size:14px;
	position:relative;
	.leftTree{
		float:left;
		width:200px;
		height:100%;
		padding: 15px;
		margin-right:30px;
		color:#fff;
		border:1px solid #054044;
		.title{
			height:40px;
			line-height:25px;
			font-size:16px;
			border-bottom:1px solid #054044;		}
		.el-tree{
			height:calc(~"(100% - 40px)");
			overflow-y:auto;
			background:none;
			color:#8a8a8a;
		}
	}
	.rightBox{
		float:left;
		width:calc(~"(100% - 230px)");
		height:100%;
		padding: 15px;
		// overflow-y:auto;
		h4{
			height:60px;
			line-height:60px;
			font-size:20px;
			text-align:center;
			color:#26f4ff;
		}
		.member{
			font-size:12px;
      color:#eee;
      width: 100%;
    height: 39rem;
    overflow: auto;
			.title{
				height:40px;
				line-height:40px;
				background:#2a3241;
				padding-left:10px;
				margin-bottom:10px;
				margin-top:10px;
				font-size:16px;
				.addBtn{
					display: inline-block;
				    text-align: center;
				    width: 30px;
				    height: 30px;
				    line-height: 30px;
				    background: #777;
				    color: #fff;
				    font-size: 20px;
				    margin-left:20px;
				    cursor:pointer;
				}
			}
			h6{
				height:35px;
				line-height:35px;
				font-size:14px;
				font-weight:600;
				margin-top:20px;
				.addBtn{
					display: inline-block;
				    text-align: center;
				    width: 30px;
				    height: 30px;
				    line-height: 30px;
				    background: #777;
				    color: #fff;
				    font-size: 20px;
				    margin-left:20px;
				    cursor:pointer;
				}
			}
			.level1{
				padding-left:30px;
				min-height:30px;
				span{
					display:inline-block;
					height:30px;
					line-height:30px;
					padding:0 15px;
					border-radius:15px;
					border:1px solid #ccc;
					min-width:50px;
					margin-left:20px;
