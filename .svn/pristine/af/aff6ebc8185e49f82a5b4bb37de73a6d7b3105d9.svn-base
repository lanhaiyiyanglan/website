.line{
	height:100%;
	width:100%;
	color:#fff;
	padding:1rem;
	font-size:14px;
	position:relative;
	.goBack{
		position:absolute;
		right:3rem;
		top:5rem;
		color:#26f4ff;
		background:#333748;
		padding:5px 15px;
		border-radius:5px;
		cursor:pointer;
	}
	.btnBox{
		height:2rem;
		margin-bottom:1rem;
		.lable{
			float:left;
			height:1.6rem;
			line-height:1.6rem;
			padding:0px 15px;
		}
		.el-select{
			float:left;
			height:1.6rem;
			line-height:1.6rem;
			margin-right:30px;
		}
		.el-select input.el-input__inner{
			height:1.6rem!important;
			line-height:1.6rem!important;
			background:#1c1e2c;
		}
		.el-date-editor--month{
			float:left;
			margin-right:30px;
			line-height:1.6rem;
		}
		.el-input__icon{
			line-height:1.6rem;			
		}
		.el-input__inner{
			background:#1c1e2c;
			height:1.6rem;
			line-height:1.6rem;
		}
		.optionsBtn{
			float:left;
			height:1.6rem;
			line-height:1.6rem;
			padding:0px 15px;
			border-radius:5px;
			background: #333748;
			cursor:pointer;
		}
		.exportBtn{
			float:left;
			height:1.6rem;
			line-height:1.6rem;
			padding:0px 15px;
			border-radius:5px;
			background: #333748;
			margin-left:20px;
			cursor:pointer;
		}
	}
	.tableBox{
		width:96%;
		margin-left:2%;
		// height:41.5rem;
		overflow:auto;
		table{
			display:inline-block;
			// width:100%;
			border-collapse:collapse;
			thead{
				display:inline-block;
				width:100%;
				tr{
					display:inline-block;
					width:100%;
					margin:0;
					padding:0;
					th{
						float:left;
						padding:0;
						margin:0;
						height:2.8rem;
						line-height:2.8rem;
						border:1px solid #1e727e;
					}
					.tableName{
						color:#26f4ff;
						height:2rem;
						line-height:2rem;
						font-size:24px;
						display:inline-block;
						padding:0;
						margin:0;
						width:100%;
            border:none;
            p{
              width: 100%;
              position: absolute;
              text-align: center;
            }
					}
					.detailName{
						color:#26f4ff;
						font-size:36px;
						height:3rem;
						line-height:3rem;
						display:inline-block;
						padding:0;
						margin:0;
						width:100%;
						border:none;
            margin-bottom:1rem;
            p{
              width: 100%;
              position: absolute;
              text-align: center;
            }
					}
				}
			}
			tbody{
				display:inline-block;
				width:100%;
				tr{
					display:inline-block;
					width:100%;
					td{
						float:left;
						padding:0;
						margin:0;
						height:1.4rem;
						line-height:1.4rem;
						border:1px solid #1e727e;
						text-align:center;
						white-space:nowrap;
					  	overflow: hidden;
					  	text-overflow: ellipsis;
					}
				}
			}
		}
	}
}
/*定义滚动条高宽及背景 高宽分别对应横竖滚动条的尺寸*/
::-webkit-scrollbar
{
    width: 8px;
    height: 8px;
    background-color: #F5F5F5;
}
 
/*定义滚动条轨道 内阴影+圆角*/
::-webkit-scrollbar-track
{
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
    border-radius: 10px;
    background-color: #F5F5F5;
}
 
/*定义滑块 内阴影+圆角*/
::-webkit-scrollbar-thumb
{
    border-radius: 10px;
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);
    background-color: #26f4ff;
}