    <script language="JavaScript" type="text/javascript">
        function addInfo(){
            var mapInfo=JSON.parse(localStorage.getItem('mapInfo'));
            var key=localStorage.getItem('pointKey');
            var optionsDay=[
                {
                    name:'1',
                    key:'one'
                },{
                    name:'2',
                    key:'two'
                },{
                    name:'3',
                    key:'three'
                },{
                    name:'4',
                    key:'four'
                },{
                    name:'5',
                    key:'five'
                },{
                    name:'6',
                    key:'six'
                },{
                    name:'7',
                    key:'seven'
                },{
                    name:'8',
                    key:'eight'
                },{
                    name:'9',
                    key:'nine'
                },{
                    name:'10',
                    key:'ten'
                },{
                    name:'11',
                    key:'eleven'
                },{
                    name:'12',
                    key:'twelve'
                },{
                    name:'13',
                    key:'thirteen'
                },{
                    name:'14',
                    key:'fourteen'
                },{
                    name:'15',
                    key:'fifteen'
                },{
                    name:'16',
                    key:'sixteen'
                },{
                    name:'17',
                    key:'seventeen'
                },{
                    name:'18',
                    key:'eighteen'
                },{
                    name:'19',
                    key:'nineteen'
                },{
                    name:'20',
                    key:'twenty'
                },{
                    name:'21',
                    key:'twenty_one'
                },{
                    name:'22',
                    key:'twenty_two'
                },{
                    name:'23',
                    key:'twenty_three'
                },{
                    name:'24',
                    key:'twenty_four'
                },{
                    name:'25',
                    key:'twenty_five'
                },{
                    name:'26',
                    key:'twenty_six'
                },{
                    name:'27',
                    key:'twenty_seven'
                },{
                    name:'28',
                    key:'twenty_eight'
                },{
                    name:'29',
                    key:'twenty_nine'
                },{
                    name:'30',
                    key:'thirty'
                },{
                    name:'31',
                    key:'thirty_one'
                }
            ]
            var dayStr='';
            for(var i=0;i<optionsDay.length;i++){
                if(key==optionsDay[i].key){
                    dayStr=optionsDay[i].name
                }
            }
            var nowTime=localStorage.getItem('nowTime');
            nowTime=nowTime.split('-');
            nowTime=nowTime[0]+'-'+nowTime[1]+'-'+dayStr;
            var pointFre=localStorage.getItem('pointFre');
            console.log(mapInfo,key,pointFre,'点');
            var addArr=[],latArr=[],longArr=[];
            mapInfo.pop();
            if(pointFre=="4"){
                for(var i=0;i<mapInfo.length;i++){
                    for(var j=0;j<mapInfo[i].pointList.length;j++){
                        var markPoint=mapInfo[i].pointList[j].AM;
                        var obj={};
                        obj.location={
                            lat:markPoint.latitude,
                            lon:markPoint.longitude
                        }  
                        obj.toolTip={
                            "tipWidth": 200, 
                            "tipHeight": 100, 
                            "content": "到达状态："+(markPoint[key] ? '已到达':'未到达')+'<br>'+'时间：'+nowTime,
                            "tipTitle": "桩号："+markPoint.pointname, 
                        }
                        obj["alpha"]= 1; 
                        obj["name"]=markPoint.pointname;
                        obj["addMouseOverListener"]=true;
                        obj["imgName"]=markPoint[key] ? "have.png":"havenot.png"; 
                        obj["addMouseClickListener"]=false;
                        obj["offsetX"]=10;
                        obj["offsetY"]=30;
                        obj["imgRotation"]=0 ; 
                        addArr.push(obj);
                        latArr.push(markPoint.latitude);
                        longArr.push(markPoint.longitude);

                        var markPoint2=mapInfo[i].pointList[j].PM;
                        var obj2={};
                        obj2.location={
                            lat:markPoint2.latitude,
                            lon:markPoint2.longitude
                        }  
                        obj2.toolTip={
                            "tipWidth": 200, 
                            "tipHeight": 100, 
                            "content": "到达状态："+(markPoint2[key] ? '已到达':'未到达')+'<br>'+'时间：'+nowTime,
                            "tipTitle": "桩号："+markPoint2.pointname, 
                        }
                        obj2["alpha"]= 1; 
                        obj2["name"]=markPoint2.pointname;
                        obj2["addMouseOverListener"]=true;
                        obj2["imgName"]=markPoint2[key] ? "have.png":"havenot.png"; 
                        obj2["addMouseClickListener"]=false;
                        obj2["offsetX"]=10;
                        obj2["offsetY"]=30;
                        obj2["imgRotation"]=0 ; 
                        addArr.push(obj2);
                        latArr.push(markPoint2.latitude);
                        longArr.push(markPoint2.longitude);
                    }                                            
                } 
            }else{
                for(var i=0;i<mapInfo.length;i++){
                    for(var j=0;j<mapInfo[i].pointList.length;j++){
                        var markPoint=mapInfo[i].pointList[j];
                        console.log(markPoint,'添加1')   
                        var obj={};
                        obj.location = {
                            lat:markPoint.latitude,
                            lon:markPoint.longitude
                        }  
                        obj.toolTip = {
                            "tipWidth": 200, 
                            "tipHeight": 100, 
                            "content": "到达状态："+(markPoint[key] ? '已到达':'未到达')+'<br>'+'时间：'+nowTime,
                            "tipTitle": "桩号："+markPoint.pointname,
                        }
                        obj.alpha = 1; 
                        obj.name = markPoint.pointname;
                        obj.addMouseOverListener = true;
                        obj.imgName = markPoint[key] ? "have.png":"havenot.png"; 
                        obj.addMouseClickListener = false;
                        obj.offsetX = 10;
                        obj.offsetY = 30;
                        obj.imgRotation = 0; 
                        addArr.push(obj);
                        latArr.push(markPoint.latitude);
                        longArr.push(markPoint.longitude);
                    }                                            
                } 
            }  
            console.log(addArr,addPointImgMarks,'添加点')  
            // var maxLat=Math.max(...latArr);
            // var minLat=Math.min(...latArr);
            // var maxLong=Math.max(...longArr);
            // var minLong=Math.min(...longArr);
            var maxLat=latArr.sort().reverse()[0];
            var minLat=latArr.sort()[0];
            var maxLong=longArr.sort().reverse()[0];
            var minLong=longArr.sort()[0];
            console.log(minLong, minLat, maxLong, maxLat,'范围')  
            zoomToExtent(minLong, minLat, maxLong, maxLat);
            //setMapCenter((minLong+maxLong)/2,(minLat+maxLat)/2)
            addPointImgMarks(JSON.stringify(addArr));                
        }
        function loadMapComplete(){
            setTimeout(function(){
                addInfo();
            },1000)
        }
        
